/* pages/carList/carList.wxss */

.add{
  background-color: #fff;
  width: 100%;
  height: 150rpx;
  position: fixed;
  top: 0rpx;
  z-index: 10;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.add .car{
  width: 90%;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #00a8ec;
  color: #fff;
  border-radius: 50rpx;
  text-align: center;
}
.content{
  margin-top: 170rpx;
}

.list{
  width: 100%;
  min-height: 210rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  background: #fff;
  border-bottom: 1rpx solid #e9eaec;
}

.list .icon{
  width: 30%;
  height: 100%;
  line-height: 210rpx;
  text-align: center;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.list .icon image{
  width: 140rpx;
  height: 140rpx;
  line-height: 210rpx;
  text-align: center;
}
.list .box image{
  width: 60rpx;
  height: 60rpx;
}

.list .box{
  width: 20%;
  height: 100%;
  line-height: 210rpx;
  text-align: center;
}

.list .txt{
  width: 50%;
  height: 200rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}

.list .txt text{
  color: #80848f;
}

.i-swipeout{
  width: 100%;
  height: 100%;
}